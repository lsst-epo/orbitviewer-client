
<div class="page-content" data-animation-type="slide">

  <div class="center__wrapper">
    <img src="{{ landing[lang].landingLogo[0].url }}" alt="{{ landing[lang].landingCenterTitle }}"> 
    <h1>{{ landing[lang].landingCenterTitle}}</h1>
    <p>{{ landing[lang].landingVersion }}</p>
  </div>

  <div class="buttons__wrapper">

    <a href="{{ '/' + lang + '/customize-orbits' | url }}" data-href="{{ '/' + lang + '/orbit-viewer' | url }}" class="ui__button orbit-button">
      <p>{{ generic[lang]['strings']['landing-explore'] }}</p>
    </a>

    <a href="{{ '/' + lang + '/guided-experiences' | url }}" class="ui__button">
      <p>{{ generic[lang]['strings']['landing-guide'] }}</p>
    </a>

  </div>

</div>

