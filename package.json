{
	"name": "orbit-viewer",
	"version": "0.0.1",
	"private": true,
	"license": "MIT",
	"scripts": {
		"server": "node ./server/server.js",
		"server-kill": "pkill node",
		"dev": "yarn glsl && eleventy --serve --watch",
		"dev-local": "yarn glsl && cross-env ELEVENTY_ENV=localhost eleventy --serve --watch",
		"build": "yarn glsl && cross-env ELEVENTY_ENV=production eleventy",
		"clean": "node ./utils/clean.js",
		"reset": "yarn clean && yarn build && yarn dev",
		"updateData": "node ./database/server.js",
		"glsl": "node utils/gen_glsl_includes"
	},
	"dependencies": {
		"@jocabola/gen": "^0.0.25",
		"@jocabola/gfx": "^0.4.91",
		"@jocabola/io": "^0.3.54",
		"@jocabola/math": "^0.2.42",
		"@jocabola/phy": "^0.1.74",
		"@jocabola/utils": "^0.1.89",
		"gsap": "^3.10.3",
		"https": "^1.0.0",
		"md5": "^2.3.0",
		"three": "^0.144.0"
	},
	"devDependencies": {
		"@11ty/eleventy": "^1.0.1",
		"@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
		"@tweakpane/core": "^1.1.0",
		"autoprefixer": "^10.4.8",
		"chokidar": "^3.5.3",
		"clean-css": "^5.3.0",
		"cross-env": "^7.0.3",
		"esbuild": "^0.15.0",
		"esbuild-plugin-alias": "^0.2.1",
		"fs-extra": "^10.1.0",
		"groq": "^2.29.3",
		"mkdirp": "^1.0.4",
		"node-fetch": "^3.2.10",
		"postcss": "^8.4.16",
		"sass": "^1.54.4",
		"tweakpane": "^3.1.0"
	}
}
